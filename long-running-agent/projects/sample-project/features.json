  "interface_contracts": {
    "UserInterface": {
      "description": "Contract for User data model",
      "methods": [
        {"name": "get_id", "returns": "string"},
        {"name": "get_email", "returns": "string"},
        {"name": "validate", "returns": "bool"}
      ]
    },
    "RepositoryInterface": {
      "description": "Contract for data persistence",
      "methods": [
        {"name": "create", "params": ["entity"], "returns": "entity"},
        {"name": "find_by_id", "params": ["id"], "returns": "entity|null"},
        {"name": "update", "params": ["entity"], "returns": "entity"},
        {"name": "delete", "params": ["id"], "returns": "bool"}
      ]
    },
    "UserServiceInterface": {
      "description": "Contract for user business logic",
      "methods": [
        {"name": "register_user", "params": ["name", "email", "password"], "returns": "User"},
        {"name": "get_user", "params": ["user_id"], "returns": "User|null"},
        {"name": "authenticate", "params": ["email", "password"], "returns": "bool"}
      ]
    }
  }
